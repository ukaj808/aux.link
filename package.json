{
  "name": "augslink-service",
  "version": "1.0.0",
  "watch": {
    "dev": {
      "patterns": [
        "app",
        "static",
        "views",
        "."
      ],
      "extensions": "js,css,html,hs"
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "dev": "webpack && cabal run -- augslink-service --env local --roomsDir . --roomsDirName rooms --public dist --home views/home.html --aw dist/audio_socket_worker_bundle.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@types/audioworklet": "^0.0.46",
    "@types/flickity": "^2.2.7",
    "@types/sortablejs": "^1.15.1",
    "copy-webpack-plugin": "^11.0.0",
    "npm-watch": "^0.11.0",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "sortablejs": "^1.15.0", "xstate": "^5.0.0-beta.13"
  }  
}
